<?xml version="1.0" encoding ="UTF-8"?>
<simulation name="Wa-Tor World model" author="team8">
    <description>The Wa-Tor World is a world filled with fish and sharks.</description>
    <parameter>
        <variable_name>SharkReproductionCycle</variable_name>
        <variable_value>6</variable_value>
    </parameter>
    <parameter>
        <variable_name>EnergyFromEatingFish</variable_name>
        <variable_value>2</variable_value>
    </parameter>
    <parameter>
        <variable_name>FishReproductionCycle</variable_name>
        <variable_value>2</variable_value>
    </parameter>
    <parameter>
        <variable_name>SharkEnergy</variable_name>
        <variable_value>3</variable_value>
    </parameter>
    <grid>
        <size>20</size>
    </grid>
    <cell>
        <cell_state>WATER</cell_state>
        <cell_number>150</cell_number>
    </cell>
    <cell>
        <cell_state>FISH</cell_state>
        <cell_number>150</cell_number>
    </cell>
    <cell>
        <cell_state>SHARK</cell_state>
        <cell_number>100</cell_number>
    </cell>
</simulation>