<?xml version="1.0" encoding="UTF-8"?>
<simulation name="Game of Life" author="team8">
    <!--This XML file sets up the initial configuration for John Horton Conway's Game of Life. Example format is shown below.-->
    <description>Conway's Game of Life is meant to act as the initial testing ground for basic Cellular Automata simulations in this project.</description>
    <grid>
        <size>20</size>
    </grid>
    <cell>
        <cell_state>ALIVE</cell_state>
        <cell_number>200</cell_number>
    </cell>
    <cell>
        <cell_state>YELLOW</cell_state>
        <cell_number>100</cell_number>
    </cell>
    <cell>
        <cell_state>RED</cell_state>
        <cell_number>100</cell_number>
    </cell>
</simulation>

